<h1>Apps#new</h1>
<p>Find me in app/views/apps/new.html.erb</p>

<div class="row">
   <div class="col-md-12">
     <%= form_for @app do |f| %>
       <div class="form-group">
         <%= f.label :app_name %>
         <%= f.text_field :app_name, class: 'form-control', placeholder: "Enter App Name" %>
       </div>
       <div class="form-group">
         <%= f.label :url %>
         <%= f.text_field :url, class: 'form-control', placeholder: "Enter URL" %>
       </div>
       <div class="form-group">
         <%= f.submit "Save", class: 'btn btn-success' %>
       </div>
     <% end %>
   </div>
 </div>