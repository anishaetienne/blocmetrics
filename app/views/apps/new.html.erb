<h1>Add New App</h1>
<div class="row">
   <div class="col-md-4">
     <%= form_for @app do |f| %>
       <div class="form-group">
         <%= f.label :App_Name %>
         <%= f.text_field :app_name, class: 'form-control', placeholder: "Enter App Name" %>
       </div>
       <div class="form-group">
         <%= f.label :Url %>
         <%= f.text_field :url, class: 'form-control', placeholder: "Enter URL" %>
       </div>
       <div class="form-group">
         <%= f.label :Descripton %>
         <%= f.text_area :url, rows: 7, class: 'form-control', placeholder: "Enter Description" %>
       </div>
       <div class="form-group">
         <%= f.submit "Save", class: 'btn btn-success' %>
       </div>
     <% end %>
   </div>
 </div>

